{
    "name": "mono-page/ext-locales",
    "description": "",
    "keywords": [
        "iiifx",
        "mono-page"
    ],
    "homepage": "https://github.com/mono-page/ext-locales",
    "type": "library",
    "authors": [
        {
            "name": "Vitaliy Khomenko",
            "email": "iiifx.me@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MonoPage\\Locales\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MonoPage\\Locales\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "process-timeout": 1800,
        "preferred-install": "dist",
        "preferred-update": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
