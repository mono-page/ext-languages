{
    "name": "mono-page/languages",
    "description": "",
    "keywords": [],
    "homepage": "https://github.com/mono-page/languages",
    "type": "library",
    "require": {
        "php": "^7.4",
        "mono-page/core": "^v0.0.1@dev"
